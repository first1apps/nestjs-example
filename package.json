{
  "private": true,
  "name": "@my/nestjs-example",
  "main": "start.js",
  "license": "UNLICENSED",
  "workspaces": [
    "src/*",
    "packages/*",
    "packages/@*/*"
  ],
  "scripts": {
    "format": "prettier --write \"**/*.ts\"",
    "build": "tsc",
    "build:watch": "tsc -w",
    "start:watch": "nodemon --ext js,json --watch config --watch packages --watch src -d 100ms --inspect=127.0.0.1:60606 ./start-dev",
    "start:dev": "nodemon",
    "prestart:prod": "rm -rf dist && tsc",
    "start:prod": "node dist/main.js",
    "start:hmr": "node dist/server",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "webpack": "webpack --config webpack.config.js"
  },
  "dependencies": {
    "@my/main-api-server": ">0.0.1",
    "@nestjs/common": "^4.6.6",
    "@nestjs/core": "^4.6.6",
    "@nestjs/microservices": "^4.6.6",
    "@nestjs/swagger": "^1.2.0",
    "@nestjs/testing": "^4.6.6",
    "@nestjs/typeorm": "^2.0.0",
    "@nestjs/websockets": "^4.6.6",
    "@types/config": "^0.0.34",
    "@types/jsonwebtoken": "^7.2.6",
    "@types/lodash": "^4.14.106",
    "@types/passport": "^0.4.3",
    "@types/passport-jwt": "^3.0.1",
    "@types/uuid": "^3.4.3",
    "class-transformer": "^0.1.9",
    "class-validator": "^0.8.5",
    "config": "^1.30.0",
    "jsonwebtoken": "^8.2.0",
    "lodash": "^4.17.5",
    "moment": "^2.22.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "pg": "^7.4.1",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^5.5.6",
    "typeorm": "^0.2.0-alpha.40",
    "typescript": "^2.8.1",
    "uuid": "^3.2.1",
    "zone.js": "^0.8.21"
  },
  "devDependencies": {
    "@types/express": "^4.0.39",
    "@types/jest": "^21.1.8",
    "@types/node": "^9.6.1",
    "@types/supertest": "^2.0.4",
    "jest": "^21.2.1",
    "nodemon": "^1.14.1",
    "prettier": "^1.11.1",
    "rimraf": "^2.6.2",
    "supertest": "^3.0.0",
    "ts-jest": "^21.2.4",
    "ts-loader": "^4.1.0",
    "ts-node": "^4.1.0",
    "tsconfig-paths": "^3.1.1",
    "tslint": "5.3.2",
    "webpack": "^4.2.0",
    "webpack-cli": "^2.0.13",
    "webpack-node-externals": "^1.6.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "packages",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage"
  }
}
